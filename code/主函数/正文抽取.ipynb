    "def str2list(s):\n",
    "    if type(s) == str:\n",
    "        imageslist = s[1:-1].split(\", \")\n",
    "        for i in range(len(imageslist)):\n",
    "            imageslist[i] = imageslist[i][1:-1]\n",
    "    if type(s) == list:\n",
    "        imageslist = s\n",
    "    return imageslist"
